{
  "all": true,
  "cache": false,
  "report-dir": "./reports/coverage",
  "temp-dir": "./reports/nyc_output",
  "check-coverage": false,
  "require": [
    "@babel/register"
  ],
  "exclude": [
    "dist/",
    "reports/",
    "src/**/*.test.js"
  ],
  "extension": [
    ".js"
  ],
  "reporter": [
    "cobertura",
    "html"
  ],
  "watermarks": {
    "statements": [50, 80],
    "lines": [50, 80],
    "functions": [50, 80],
    "branches": [50, 80]
  }
}
